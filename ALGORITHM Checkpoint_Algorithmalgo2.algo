ALGORITHM Checkpoint_Algorithm
VAR

ch:CHAR;
l:INTEGER:=0;
nbv:INTEGER:=0;
nbw:INTEGER:=0;

BEGIN
 REPEAT
     Read(ch);
     l=l+1;
     IF ((ch=" ") or (ch=".")) THEN
     nbw=nbw+1;
     
     ELSE IF (ch in ['a','o','e','u','i','y','A','O','E','U','I','Y']) THEN
     nbv=nbv+1;
     
     END_IF
 UNTIL (ch=".");

                                         

END